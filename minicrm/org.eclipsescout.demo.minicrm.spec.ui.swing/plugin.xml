<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="app"
         point="org.eclipse.core.runtime.applications">
      <application cardinality="singleton-global" thread="main" visible="true">
         <run class="org.eclipsescout.demo.minicrm.spec.ui.swing.MinicrmSpecApplication"/>
      </application>
   </extension>
   <extension id="product" point="org.eclipse.core.runtime.products">
      <product application="org.eclipsescout.demo.minicrm.spec.ui.swing.app" name="MiniCRM Spec Client">
         <property
               name="appName"
               value="MiniCRM Spec Client">
         </property>
      </product>
   </extension>
</plugin>
